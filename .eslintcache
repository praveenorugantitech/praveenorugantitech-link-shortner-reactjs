> {\n    // Regex to check if string is a valid URL\n    return string.match(HTTP_URL_VALIDATOR_REGEX);\n  };\n\n  // Function that calls the API if link is valid\n  const getLink = async () => {\n    await shrtcode\n      .get(`shorten?url=${link}`)\n      .then((response) => {\n        setShort(response.data.result.short_link);\n        setLoading(false);\n      })\n      .catch((error) => {\n        console.error(error);\n      });\n  };\n\n  return (\n    <>\n      <StyledForm onSubmit={(e) => handleSubmit(e)} autoComplete='off'>\n        {/* <Label htmlFor='input'>Long Link Here:</Label> */}\n        <TextField\n          style={{ marginBottom: '20px' }}\n          label='Link'\n          variant='outlined'\n          value={link}\n          onChange={(e) => setLink(e.target.value)}\n        />\n\n        {!loading && (\n          <Button\n            onClick={(e) => handleSubmit(e)}\n            variant='contained'\n            color='primary'\n          >\n            Submit\n          </Button>\n        )}\n\n        {loading && <LinearProgress />}\n      </StyledForm>\n      {short && (\n        <>\n          <h2>Short Link: </h2>\n\n          <DisplayLink shortend={short} />\n        </>\n      )}\n    </>\n  );\n};\n\nexport default Search;\n","D:\\praveenorugantitech\\projects\\Git\\praveenorugantitech-reactjs\\0_Projects\\praveenorugantitech-link-shortner\\src\\components\\Container.js",[],"D:\\praveenorugantitech\\projects\\Git\\praveenorugantitech-reactjs\\0_Projects\\praveenorugantitech-link-shortner\\src\\components\\DisplayLink.js",[],"D:\\praveenorugantitech\\projects\\Git\\praveenorugantitech-reactjs\\0_Projects\\praveenorugantitech-link-shortner\\src\\apis\\shrtcode.js",[],["37","38"],{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","severity":1,"message":"44","line":15,"column":76,"nodeType":"45","messageId":"46","endLine":15,"endColumn":77,"suggestions":"47"},{"ruleId":"43","severity":1,"message":"44","line":1